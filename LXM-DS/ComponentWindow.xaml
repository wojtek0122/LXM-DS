<Window x:Class="LXM_DS.ComponentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LXM_DS"
        mc:Ignorable="d"
        Title="COMPONENT" Height="1080" Width="1920"
        WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <Button x:Name="OK" HorizontalAlignment="Left" Height="60" Margin="125,500,0,0" VerticalAlignment="Top" Width="200" Click="OK_Click"/>
        <Button x:Name="NOK" HorizontalAlignment="Left" Height="66" Margin="475,500,0,0" VerticalAlignment="Top" Width="200" Click="NOK_Click"/>
        <WebBrowser x:Name="_browser" HorizontalAlignment="Left" Height="485" Margin="0,0,-22,0" VerticalAlignment="Top" Width="814" IsEnabled="False"/>
    </Grid>

</Window>
